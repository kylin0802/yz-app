.examinee-page {
  &-list {
    box-shadow: 0 5px 8px 0 #DAE1E5;
    margin-bottom: 20px;
    &-operation {
      height: 96px;
      line-height: 96px;
      padding: 0 5px;
      .ant-btn{
        margin: 0 10px;
      }
      &-left {
        display: inline-block;
        float: left;
        text-align: left;
        margin-left: 28px;

        > button {
          margin: 0 12px;
          color: #399CFF;
          border-color: #399CFF;
        }
      }

      &-right {
        display: inline-block;
        float: right;
        margin-right: 76px;
        text-align: right;

        > button {
          margin: 0 12px;
          color: #399CFF;
          border-color: #399CFF;
        }
      }
    }

    &-content {
      padding: 0 5px;
      .ant-table-tbody {
        text-align: center;
      }

      .ant-table-thead > tr > th {
        text-align: center;
        border: none;
        background: rgba(243, 247, 255, 1);
        //font-size: 18px;
      }

      .ant-table-tbody > tr > td {
        border: none;
        //font-size: 18px;
      }

      .ant-table-tbody > tr:nth-child(even) {
        background: rgba(243, 247, 255, 1);
      }

    }

    &-pagination {
      padding: 10px 0;
      text-align: right;
      padding-right: 100px;
      //border-top: 1px solid #eeeeee;
    }
  }
}

