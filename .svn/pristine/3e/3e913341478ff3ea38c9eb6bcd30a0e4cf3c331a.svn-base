.headerSearch-page-search {
    margin:  0;
    padding: 24px 32px 4px;
    box-shadow: 0 5px 8px 0 #DAE1E5;
    position: relative;
    &-label {
      display: inline-block;
      margin-right: 25px;
      margin-bottom: 20px;
      > label {
        margin-right: 7px;
        color: #69758C;
      }
      > span{
        display: inline-block;
      }
    }
    &-list{
      display: inline-block;
    }
    &-buttons {
      position: absolute;
      top: 5px;
      right: -25px;
      > button{
        margin: 0 12px;
      }
    }
  }
  
  .w200{
    width: 200px;
  }
  
  .w250{
    width: 250px !important;
  }
  .headerSearch-page-search-label-span{
    display: inline-block;
      width: 147px;
      margin-left: 10px;
  }
  