.verificationDatabase-page{
    &-header{
        box-shadow: 0 5px 8px 0 #DAE1E5;
        margin-bottom: 10px;
        margin: 20px 0;
        padding: 24px 32px 4px;
    }
    &-content{
      box-shadow: 0 5px 8px 0 #DAE1E5;
      padding:0 5px;
        .ant-table-tbody {
            text-align: center;
          }
    
          .ant-table-thead > tr > th {
            text-align: center;
            border: none;
            background: rgba(243, 247, 255, 1);
            //font-size: 18px;
          }
    
          .ant-table-tbody > tr > td {
            border: none;
            //font-size: 18px;
          }
    
          .ant-table-tbody > tr:nth-child(even) {
            background: rgba(243, 247, 255, 1);
          }
        &-button{
            margin-right: 5px;
            cursor: pointer;
        }
    }
}