.examineeCheck-page {
  &-list {
    box-shadow: 0 5px 8px 0 #dae1e5;
    margin-bottom: 20px;
    &-operation {
      height: 70px;
      line-height: 70px;
      // display: flex;
      // align-content: space-between;
      // padding: 20px 5px 20px 10px;
      padding: 0 15px;
      .ant-btn {
        margin: 0 15px;
      }
      &-left {
        display: inline-block;
        float: left;
        text-align: left;
        margin-left: 28px;
        > button {
          margin: 0 12px;
          color: #399cff;
          border-color: #399cff;
        }
      }
      &-right {
        display: inline-block;
        float: right;
        margin-right: 76px;
        text-align: right;

        > button {
          margin: 0 12px;
          color: #399cff;
          border-color: #399cff;
        }
      }
    }

    &-content {
      padding: 10px;
      max-height: 580px;
      overflow: auto;
      background: rgba(243, 247, 255, 1);
      border: 1px solid rgba(223, 226, 232, 1);
      .ant-table-tbody {
        text-align: center;
      }
      &-wrapper{
        padding: 12px;
      }
      display: flex;
      flex-wrap:wrap;
      // justify-content: space-between;
    }
    &-pagination {
      padding: 10px 0;
      text-align: right;
      padding-right: 100px;
      //border-top: 1px solid #eeeeee;
    }
  }
}
