import React from 'react'
// import Form from '@/components/Form' // 样式全局配
import Dialog, { withLazy } from '@/components/Dialog'
import styles from './index.module.less'
import { Row, Col } from 'antd'
import noAvatar from '@/images/test.jpg'



const Wrap = props => {
  const { examineeData } = props
  let imgUrl = !! examineeData.photoPath ? `/yzSmartGate/common/${examineeData.photoPath}` : noAvatar
  return (
    <section className={styles.wrap}>
      <div className={styles.moduleLeft}>
        <div className={styles.moduleLeftTitle}>照片详情</div>
        <img className={styles.moduleImage} src={imgUrl} alt="照片详情"/>
      </div>
      <Row className={styles.moduleRight}>
        <Col span={24} className={styles.moduleLeftCol}>
          <span className={styles.moduleLeftColSpan}>考生编号：</span>
          <span className={styles.moduleLeftColItem}>{examineeData.examineeId}</span>
        </Col>
        <Col span={24} className={styles.moduleLeftCol}>
          <span className={styles.moduleLeftColSpan}>联系方式：</span>
          <span className={styles.moduleLeftColItem}>{examineeData.phone}</span>
        </Col>
        <Col span={24} className={styles.moduleLeftCol}>
          <span className={styles.moduleLeftColSpan}>身份证号：</span>
          <span className={styles.moduleLeftColItem}>{examineeData.idCard}</span>
        </Col>
        <Col span={24} className={styles.moduleLeftCol}>
          <span className={styles.moduleLeftColSpan}>图片路径：</span>
          <span className={styles.moduleLeftColItem}>{examineeData.photoPath}</span>
        </Col>
        <Col span={24} className={styles.moduleLeftCol}>
          <span className={styles.moduleLeftColSpan}>考生性别：</span>
          <span className={styles.moduleLeftColItem}>{examineeData.sex}</span>
        </Col>
        <Col span={24} className={styles.moduleLeftCol}>
          <span className={styles.moduleLeftColSpan}>座位号：</span>
          <span className={styles.moduleLeftColItem}>{examineeData.seatNo}</span>
        </Col>
      </Row>
    </section>

  )
}



const ExamineeDetail = props => {
  return (<Dialog title="详情" {...props}>
    <Wrap {...props}></Wrap>
  </Dialog>)
}

ExamineeDetail.defaultProps = {
  width: '600px'
}

export default withLazy(ExamineeDetail)


