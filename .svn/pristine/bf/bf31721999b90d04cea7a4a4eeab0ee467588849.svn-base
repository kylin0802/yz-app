import React from 'react';

import { Form } from 'antd';

import classNames from 'classnames';

import './index.less';

export default function YzForm(props) {
  let { className, ...rest } = props;
  return <Form {...rest} className={classNames('yz-form', className)} />;
}

YzForm.Item = Form.Item;
YzForm.create = Form.create;
