.wrap {
  margin: 4px;
  border: 1px solid #ddd;
  font-size: 14px;
  width: 310px;
  height: 265px;
  display: flex;
  background: #fff;
  justify-content: center;
  // border: 1px solid #ddd;
}

.left,
.right{
  display: flex;
  flex-direction: column;
  // border: 2px solid black;
  padding: 10px;
}
.leftTitle, .rightTitle{
  color: #606266;
  font-weight: 200;
  display: block;
  text-align: center;
  line-height: 30px;
}
.leftImage, .rightImage{
  width: 103px;
  height: 138px;
}
.leftCol, .rightCol{
  color: #909399;
}
.success {
 
color:#3A9CFF ;
}
.error{
  color: #F45A57;
  background:rgba(255,242,242,1);

}
.btn {
  width:89px;
  height:31px;
  background:rgba(243,247,255,1);
}
.leftColItem, .rightColItem{}
