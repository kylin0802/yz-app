import React, { useState, useEffect } from "react";
import fetch from "@/services/axios";
import { get, toString } from "lodash";
import { Button } from "antd";
import { EXAMINEE_CHECK_LIST_API, EXAMINEE_CHECK_ACTION_API } from "./apis";
import "./index.less";
import Search from "./components/Search";
import CheckDialog from "./components/CheckDialog";
import CardList from "./components/CardList";

function ExamineeCheck() {
  const [cardList, setCardList] = useState([]);
  const [pagination, setPagination] = useState({
    examNumberId: "",
    passResult: "",
      pageNumber: 0,
    pageSize: 100
  });

 

  useEffect(() => {
    // console.log('更新', JSON.stringify(pagination))
    getList()
  }, [JSON.stringify(pagination)])

  const onHandleSearch = params => {
    // console.log(params)
    
    setPagination({...pagination, ...params})
   
  };

  const getList = params => {
    fetch
      .post(EXAMINEE_CHECK_LIST_API, {...pagination})
      .then(res => {
        console.log(get(res, 'state'))
        let val = get(res, 'state') === -1 ? [] : get(res, 'data.content')
        console.log(val)
        setCardList(val)
      });
  }

  return (
    <>
      <section className="examineeCheck-page">
        <section className="examineeCheck-page-header">
          <Search onHandleSearch={onHandleSearch} />
        </section>
        <section className="examineeCheck-page-list">
          <section className="examineeCheck-page-list-operation">
            <span className="examineeCheck-page-list-span">当前考场</span>
            <CheckDialog
              checkAPI={EXAMINEE_CHECK_ACTION_API}
              params={pagination}
              onReload={onHandleSearch}
            >
              <Button
                type="primary"
                disabled={!pagination.examNumberId}
                size="small"
              >
                重新校验
              </Button>
            </CheckDialog>
          </section>
          <section className="examineeCheck-page-list-content-wrapper">
            <section className="examineeCheck-page-list-content">
              <CardList cardList={cardList} />
            </section>
          </section>
        </section>
      </section>
    </>
  );
}

export default ExamineeCheck;
