.examRoom-page{
    
    &-header{
        margin: 20px 0;
        padding: 24px 32px 4px;
        box-shadow: 0 5px 8px 0 #DAE1E5;
    }
    &-content{
        box-shadow: 0 5px 8px 0 #DAE1E5;
        padding:0 5px;
        
        &-bread{
            height: 70px;
            line-height: 70px;
            padding: 0 5px;
            &-span{
                font-size: 18px;
            }
            Button{
                float:left;
                margin:10px 10px 0px 10px;
            }
        
        }
        &-table{
            .ant-table-tbody {
                text-align: center;
              }       
              .ant-table-thead > tr > th {
                text-align: center;
                border: none;
                background: rgba(243, 247, 255, 1);
              }      
              .ant-table-tbody > tr > td {
                border: none;
              }  
              .ant-table-tbody > tr:nth-child(even) {
                background: rgba(243, 247, 255, 1);
              }
            &-text{
             color: #3A9CFF;
             cursor: pointer;
            }
            &-f{
              margin:0 3px;
            }
        }
    }
}